            locale: moment.locale('zh-cn'),
            showClose: true

        });
        $("#leaveTime").datetimepicker({
            format: 'YYYY-MM-DD HH:mm',
            locale: moment.locale('zh-cn'),
            showClose: true
        });
        $("#useTime").datetimepicker({
            format: 'YYYY-MM-DD HH:mm',
            locale: moment.locale('zh-cn'),
            allowInputToggle: true,
            showClose: true
        });

    });

</script>
