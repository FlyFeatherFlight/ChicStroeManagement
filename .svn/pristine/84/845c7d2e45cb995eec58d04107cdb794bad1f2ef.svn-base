                                        <li><a href="@Url.Action("DesignResultExamineIndex", "DesignerExamine")" class=" hvr-bounce-to-right"><i class="fa fa-question-circle nav_icon"></i>设计案完结审查</a></li>

                                    </ul>
                                </li>
                            }
                            @if (ViewBag.IsManager == true)
