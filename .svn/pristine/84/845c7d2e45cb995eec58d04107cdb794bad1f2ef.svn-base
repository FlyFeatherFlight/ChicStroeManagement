    @*时间日期插件*@
    <script src="~/Scripts/laydate/laydate.js"></script>
    <script>
        //同时绑定多个时间
        lay('.timeLay').each(function () {
            laydate.render({
                elem: this,
                type: 'time'
                , trigger: 'click'
            });
        });
       
    
        //同时绑定多个日期
        lay('.dateLay').each(function () {
            laydate.render({
                elem: this
                , trigger: 'click'
            });
        });
    </script>
